{
  "name": "@codebakery/origami",
  "version": "1.1.2",
  "description": "Angular + Polymer",
  "keywords": [
    "Angular",
    "Polymer"
  ],
  "author": "Andrew Mitchell <abmitchell77@gmail.com>",
  "license": "MIT",
  "main": "./bundles/origami.umd.js",
  "module": "./@codebakery/origami.js",
  "es2015": "./@codebakery/origami.es5.js",
  "typings": "./origami.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hotforfeature/origami.git"
  },
  "homepage": "https://github.com/hotforfeature/origami#readme",
  "bugs": {
    "url": "https://github.com/hotforfeature/origami/issues"
  },
  "scripts": {
    "clean": "rimraf out-tsc/ dist/ coverage/ test/build/",
    "lint": "tslint --project src/tsconfig.json",
    "bower": "bower install",
    "test": "karma start",
    "test:watch": "karma start --auto-watch --no-single-run",
    "pretest:es5": "cd test && polymer build",
    "test:es5": "ES5=true karma start",
    "precommit": "npm run lint",
    "commitmsg": "validate-commit-msg",
    "commit": "git-cz",
    "prebuild": "npm run clean",
    "build": "node build.js",
    "release": "standard-version"
  },
  "peerDependencies": {
    "@angular/core": ">=4.0.0 <5.0.0",
    "@angular/forms": ">=4.0.0 <5.0.0",
    "@angular/platform-browser": ">=4.0.0 <5.0.0",
    "rxjs": "^5.0.1"
  },
  "devDependencies": {
    "@angular/common": "^4.1.3",
    "@angular/compiler": "^4.1.3",
    "@angular/compiler-cli": "^4.1.3",
    "@angular/core": "^4.1.3",
    "@angular/forms": "^4.1.3",
    "@angular/platform-browser": "^4.1.3",
    "@angular/platform-browser-dynamic": "^4.1.3",
    "@types/jasmine": "^2.5.51",
    "@types/node": "^7.0.29",
    "awesome-typescript-loader": "^3.1.3",
    "bower": "^1.8.0",
    "camelcase": "^4.1.0",
    "codelyzer": "^3.0.1",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "glob": "^7.1.2",
    "husky": "^0.13.4",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^2.6.3",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.3",
    "karma-safari-launcher": "^1.0.0",
    "karma-sauce-launcher": "^1.1.0",
    "karma-webpack": "^2.0.3",
    "polymer-cli": "^1.1.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.42.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^2.0.1",
    "rxjs": "^5.4.0",
    "source-map-loader": "^0.2.1",
    "standard-version": "^4.0.0",
    "tslint": "^5.4.3",
    "typescript": "^2.3.4",
    "validate-commit-msg": "^2.12.1",
    "webpack": "^2.6.1",
    "zone.js": "^0.8.12"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
